# Find the real .lib/.a and add them to opus_LIBS and opus_LIBRARY_LIST
set(opus_LIBRARY_LIST opus m)
set(opus_LIB_DIRS "/home/alex/.conan/data/opus/1.3.1/bincrafters/stable/package/6af9cc7cb931c5ad942174fd7838eb655717c709/lib")
foreach(_LIBRARY_NAME ${opus_LIBRARY_LIST})
    unset(CONAN_FOUND_LIBRARY CACHE)
    find_library(CONAN_FOUND_LIBRARY NAME ${_LIBRARY_NAME} PATHS ${opus_LIB_DIRS}
                 NO_DEFAULT_PATH NO_CMAKE_FIND_ROOT_PATH)
    if(CONAN_FOUND_LIBRARY)
        list(APPEND opus_LIBRARIES ${CONAN_FOUND_LIBRARY})
        if(NOT ${CMAKE_VERSION} VERSION_LESS "3.0")
            # Create a micro-target for each lib/a found
            set(_LIB_NAME CONAN_LIB::opus_${_LIBRARY_NAME})
            if(NOT TARGET ${_LIB_NAME})
                # Create a micro-target for each lib/a found
                add_library(${_LIB_NAME} UNKNOWN IMPORTED)
                set_target_properties(${_LIB_NAME} PROPERTIES IMPORTED_LOCATION ${CONAN_FOUND_LIBRARY})
            else()
                message(STATUS "Skipping already existing target: ${_LIB_NAME}")
            endif()
            list(APPEND opus_LIBRARIES_TARGETS ${_LIB_NAME})
        endif()
        message(STATUS "Found: ${CONAN_FOUND_LIBRARY}")
    else()
        message(STATUS "Library ${_LIBRARY_NAME} not found in package, might be system one")
        list(APPEND opus_LIBRARIES_TARGETS ${_LIBRARY_NAME})
        list(APPEND opus_LIBRARIES ${_LIBRARY_NAME})
    endif()
endforeach()
set(opus_LIBS ${opus_LIBRARIES})

if(NOT ${CMAKE_VERSION} VERSION_LESS "3.0")
    # Target approach
    if(NOT TARGET opus::opus)
        add_library(opus::opus INTERFACE IMPORTED)
        
    if(opus_INCLUDE_DIRS)
      set_target_properties(opus::opus PROPERTIES INTERFACE_INCLUDE_DIRECTORIES "${opus_INCLUDE_DIRS}")
    endif()
    set_property(TARGET opus::opus PROPERTY INTERFACE_LINK_LIBRARIES ${opus_LIBRARIES_TARGETS} "${opus_LINKER_FLAGS_LIST}")
    set_property(TARGET opus::opus PROPERTY INTERFACE_COMPILE_DEFINITIONS ${opus_COMPILE_DEFINITIONS})
